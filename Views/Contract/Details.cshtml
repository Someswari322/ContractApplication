<!-- Views/Contract/Details.cshtml -->
@model ContractApplication.Models.Contract

<h2>Contract Details</h2>

<div>
    <h4>Contract</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Contract Name
        </dt>
        <dd class="col-sm-10">
            @Model.ContractName
        </dd>
        <dt class="col-sm-2">
            Client Name
        </dt>
        <dd class="col-sm-10">
            @Model.ClientName
        </dd>
    </dl>
</div>

<div>
    <a asp-action="Edit" asp-route-partitionKey="@Model.PartitionKey" asp-route-rowKey="@Model.RowKey">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
